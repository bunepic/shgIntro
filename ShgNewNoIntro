function trans1()
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Right Arm"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Head"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Left Arm"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Torso"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Ears"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Mask"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["TorsoDecor"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Eye"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Belt"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["HieroHead"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["EyeOutlines"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["HeadBackDecor"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["LArmDecor"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["RArmDecor"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["RGlove"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["LGlove"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["ShoulderLining"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["MiddleHeadStripe"],1)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["SideHeadStripes"],1)
end
