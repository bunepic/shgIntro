                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

function humanbarrage()
	if death == true then return end
	if active == true then return end
	active = true
	local bep = true
	local de = mouse.KeyUp:connect(function(key)
	    if key == "e" then
	      bep = false
	    end
	end)
	_G.anim29:Play(0.1,1,999)
	repeat
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.5,0.1,-0.5), Vector3.new(-0.5,0.1,-0.5), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.3,0.1,-0.3), Vector3.new(-0.3,0.1,-0.3), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.1,0.1,-0.1), Vector3.new(-0.1,0.1,-0.1), BrickColor.new("Bright green"))
		_G.playerbarrageone()
		_G.playerbarragetwo()
		wait(0.000005)
	until bep == false
	_G.anim29:Stop()
	active = false
end

mouse.KeyDown:connect(function(key)
	if key == "e" then
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		humanbarrage()
	end
end)
