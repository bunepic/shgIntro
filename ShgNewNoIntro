	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 50 + Vector3.new(0, -25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 50 + Vector3.new(0, -25, 0), 0.08, "", 1, 1)
