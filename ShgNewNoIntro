-----------------------------------------------
local beatdown = false

local beatdowntarget = nil

_G.beatdowngrab = function()
    local leftArm = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"]

    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = leftArm.CFrame
    hitbox1.Transparency = 1
    hitbox1.Name = "hitbox1"
    hitbox1.CanCollide = false
    hitbox1.Anchored = false
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    hitbox1.Size = leftArm.Size * 1.5

    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = leftArm

    game.Debris:AddItem(hitbox1, 0.3)

    local debounce = false
    
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
            if debounce == false then
            debounce = true
            print(hit.Parent.Name)
            delay(.001, function() debounce = false end)
        beatdown = true
        beatdowntarget = hit.Parent
        game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
        game.ReplicatedStorage.KQRGrab:FireServer(hit.Parent.Humanoid)
        hitbox1:Destroy()
    end
elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
    if debounce == false then
        debounce = true
        print(hit.Parent.Name)
        delay(.001, function() debounce = false end)
        beatdown = true
        beatdowntarget = hit.Parent
        game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
        game.ReplicatedStorage.KQRGrab:FireServer(hit.Parent.Humanoid)
    hitbox1:Destroy()
    end
end
end)
end
-------------------------------------------
local beatdowncountthing = 0

_G.beatdown = function()
    repeat
        beatdowncountthing = beatdowncountthing + 1
        game.ReplicatedStorage.Knock:FireServer(beatdowntarget.Humanoid)
        game.ReplicatedStorage.KQRGrab:FireServer(beatdowntarget.Humanoid)
        game.ReplicatedStorage.Berserk:FireServer(beatdowntarget.Humanoid)
        game:GetService("ReplicatedStorage").Damage12Sans:FireServer(beatdowntarget.Humanoid, beatdowntarget.HumanoidRootPart.CFrame, 3, 0, Vector3.new(0, 0, 0), 0.055, "", 1, 1)
        game:GetService("ReplicatedStorage").Damage2:FireServer(beatdowntarget.Humanoid, beatdowntarget.HumanoidRootPart.CFrame, 3, 0, Vector3.new(0, 0, 0), 0.055, "", 1, 1)
        wait()
    until beatdowncountthing == 500 or beatdowntarget.Humanoid.Health == 50 or beatdowntarget.Humanoid.Health < 50
    beatdowncountthing = 0
end
