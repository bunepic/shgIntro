            wait(0.1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 99, 0.05, Vector3.new(math.random(-20,20),math.random(-20,20),math.random(-20,20)), 0.01, "rbxassetid://5382576606", 1, 1)
