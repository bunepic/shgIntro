function trans0()
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Right Arm"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Head"],0,5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Left Arm"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Stand Torso"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Ears"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Mask"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["TorsoDecor"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Eye"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["Belt"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["HieroHead"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["EyeOutlines"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["HeadBackDecor"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["LArmDecor"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["RArmDecor"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["RGlove"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["LGlove"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["ShoulderLining"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["MiddleHeadStripe"],0.5)
    game.ReplicatedStorage.Transparency:FireServer(game.Players.LocalPlayer.Character.Stand["SideHeadStripes"],0.5)
end
