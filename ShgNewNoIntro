	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
