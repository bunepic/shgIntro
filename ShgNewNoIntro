_G.omtohScream = function()
    game:GetService("ReplicatedStorage").RTZEffect:FireServer(true)
    yell:FireServer()
    wait(0.275)
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(1, -0.3, 1), Vector3.new(1, -0.3, 1), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(2, -0.5, 2), Vector3.new(2, -0.5, 2), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(3, -0.7, 3), Vector3.new(3, -0.7, 3), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(4, -1, 4), Vector3.new(4, -1, 4), BrickColor.new("Bright green"))
    doppioslam:FireServer()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 50 then
            game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game.ReplicatedStorage.SamuraiDamage2:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            end
        end
    end
end
-----------------------------------------------
mouse.KeyDown:connect(function(key)
	if key == "t" then
	if death == true then return end
	if active == true then return end
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		active = true
		_G.anim28:Play()
		wait(0.4)
		_G.omtohScream()
		wait(0.5)
		active = false
	end
end)
-----------------------------------------------
_G.barrageone = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].Size * 1.2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.1) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 15, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 15, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

_G.playerbarrageone = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 1.2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.1) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.05, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

