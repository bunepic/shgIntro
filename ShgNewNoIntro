		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(2, -1, 2), Vector3.new(2, -1, 2), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(3, -1.5, 3), Vector3.new(3, -1.5, 3), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(4, -2, 4), Vector3.new(4, -2, 4), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(5, -2.5, 5), Vector3.new(5, -2.5, 5), BrickColor.new("Bright green"))
		wait(0.1)
		_G.anim30:AdjustSpeed(1)
		wait(0.8)
		megapunchactiveautra = true
        wait(0.4)
		active = false
	end
end)
----------------------
local magmaTime = 0

local magmactive = false

local Objs = {}
local LastBottomObj
local Model = Instance.new("Model")
Model.Name = "Obj"
Model.Parent = Character
local nonoMainObj = Instance.new("Part")
nonoMainObj.Anchored = true
nonoMainObj.CanCollide = false
nonoMainObj.Massless = true
nonoMainObj.Transparency = 1
nonoMainObj.Size = Vector3.new(1,1,1)
nonoMainObj.Parent = Model

local NotMainObj = Instance.new("Part")
NotMainObj.Anchored = true
NotMainObj.CanCollide = false
NotMainObj.Massless = true
NotMainObj.Transparency = 1
NotMainObj.Size = Vector3.new(1,1,1)
NotMainObj.Parent = Model

local NotMainObj2 = Instance.new("Part")
NotMainObj2.Anchored = true
NotMainObj2.CanCollide = false
NotMainObj2.Massless = true
NotMainObj2.Transparency = 1
NotMainObj2.Size = Vector3.new(1,1,1)
NotMainObj2.Parent = Model

local NotNotMainObj = Instance.new("Part")
NotMainObj.Anchored = true
NotMainObj.CanCollide = false
NotMainObj.Massless = true
NotMainObj.Transparency = 1
NotMainObj.Size = Vector3.new(1,1,1)
NotMainObj.Parent = Model

local NotNotMainObj2 = Instance.new("Part")
NotMainObj2.Anchored = true
NotMainObj2.CanCollide = false
NotMainObj2.Massless = true
NotMainObj2.Transparency = 1
NotMainObj2.Size = Vector3.new(1,1,1)
NotMainObj2.Parent = Model

mouse.KeyDown:connect(function(key)
	if key == "4" then
    if magmactive == true then return end
	if death == true then return end
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		magmactive = true
		nonoMainObj.CFrame = hrp.CFrame * CFrame.new(0,-40,0)
		NotMainObj.CFrame = hrp.CFrame * CFrame.new(40,-40,0)
		NotMainObj2.CFrame = hrp.CFrame * CFrame.new(-40,-40,0)
		NotNotMainObj.CFrame = hrp.CFrame * CFrame.new(0,-40,40)
		NotNotMainObj2.CFrame = hrp.CFrame * CFrame.new(0,-40,-40)
		repeat
 		   magmaTime = magmaTime + 1
 		   _G.magmaearthquake()
 		   game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotNotMainObj2.CFrame, 0, 0, nil, 0.65, "", 1, 0)
 		   game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotNotMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
 		   game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotMainObj2.CFrame, 0, 0, nil, 0.65, "", 1, 0)
 		   game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, NotMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
   		   game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, nonoMainObj.CFrame, 0, 0, nil, 0.65, "", 1, 0)
   		   wait()
		until human.Health == 0 or human.Health < 0 or magmaTime == 200
        nonoMainObj:Destroy()
        NotMainObj:Destroy()
        NotNotMainObj:Destroy()
        NotMainObj2:Destroy()
        NotNotMainObj2:Destroy()
		magmaTime = 0
		wait(0.5)
		magmactive = false
	end
end)

_G.magmaearthquake = function()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 150 then
            game:GetService("ReplicatedStorage").Knock:FireServer(v.Humanoid)
            game:GetService("ReplicatedStorage").BurnDamage:FireServer(v.Humanoid,v.HumanoidRootPart.CFrame,0,0,Vector3.new(0,0,0),"rbxassetid://241837157",0,Color3.new(255, 255, 255),"",0,0)
            end
        end
    end
end
-----------------------------------------------
local blackholeVoidSize = 0

local blackholeSize = 0

local blackholeTime = 0

local blackholeactive = false

local Objs = {}
local LastBottomObj
local Model = Instance.new("Model")
Model.Name = "Obj"
Model.Parent = Character
local MainObj = Instance.new("Part")
MainObj.Anchored = true
MainObj.CanCollide = false
MainObj.Massless = true
MainObj.Transparency = 1
MainObj.Size = Vector3.new(1,1,1)
MainObj.Parent = Model

mouse.KeyDown:connect(function(key)
	if key == "7" then
    if blackholeactive == true then return end
	if death == true then return end
    if loading == true then return end
    if blackholeactive == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		blackholeactive = true
		MainObj.CFrame = hrp.CFrame * CFrame.new(0,25,0)
		repeat
 		   blackholeVoidSize = blackholeVoidSize + 60
 		   blackholeTime = blackholeTime + 1
  		   blackholeSize = blackholeSize + 0.01
 		   _G.blackholeVoid()
   		   game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, MainObj.CFrame, 0, 0, nil, blackholeSize, "", 1, 0)
   		   wait()
		until human.Health == 0 or human.Health < 0 or blackholeTime == 400
        MainObj:Destroy()
		blackholeTime = 0
		blackholeSize = 0
		wait(0.5)
		blackholeactive = false
	end
end)

_G.blackholeVoid = function()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < blackholeVoidSize then
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 2.5, 0.1, ((MainObj.Position+MainObj.CFrame.LookVector*0)-v.HumanoidRootPart.Position)*1, 0, "rbxassetid://4137216950", 0.8, 0)
            end
        end
    end
end
-----------------------------------------------
_G.kcChoppa = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].Size * 2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.3) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
	    	local limbuchoppa = "limbchop"
	    	local limbuchoppa2 = 
{
	    	[1] = hit.Parent.Parent.Humanoid, 
	    	[2] = hit.Parent.Parent.Torso.CFrame
}
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
	    	local limbuchoppa = "limbchop"
	    	local limbuchoppa2 = 
{
	    	[1] = hit.Parent.Parent.Humanoid, 
	    	[2] = hit.Parent.Parent.Torso.CFrame
}
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            hitbox1:Destroy()
            end
        end
    end)
end

mouse.KeyDown:Connect(function(key)
    if key == 'e' then
	if death == true then return end
	if secretmodeon == false then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
		trans0()
		active = true
        _G.anim36:Play(0.1, 1, 1.25)
        wait(0.45)
        _G.kcChoppa()
		trans1()
        wait(1)
        active = false
    end 
end) 

_G.kcbullet = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"].Size * 2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.3) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game.ReplicatedStorage.KQRGrab:FireServer(hit.Parent.Humanoid)
	    	game.ReplicatedStorage.Knock:FireServer(hit.Parent.Humanoid)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame * CFrame.new(0,1,0), 99, 0, Vector3.new(0, 0, 0), 0.025, "", 1, 1)
