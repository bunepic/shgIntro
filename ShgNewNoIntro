_G.specialdashmove = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.5) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(3, -3, 3), Vector3.new(3, -3, 3), BrickColor.new("Bright green"))
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            wait(0.5)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(3, -3, 3), Vector3.new(3, -3, 3), BrickColor.new("Bright green"))
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 99, 0.1, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent.Head.CFrame, 99, 0, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
            wait(0.5)
            hitbox1:Destroy()
            end
        end
    end)
end
-----------------------------------------------
local noantifling = false

mouse.KeyDown:Connect(function(key)
    if key == 'g' then
    if standmodecooler == false then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
	if death == true then return end
        active = true
        Humanoid.WalkSpeed = 0
        Humanoid.JumpPower = 0
        _G.anim24:Play(0.1, 1, 1.6)
        wait(0.6)
        noantifling = true
        game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(1, 1, 1), Vector3.new(1, 1, 1), BrickColor.new("Bright green"))
        game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(0.7, 1, 0.7), Vector3.new(0.7, 1, 0.7), BrickColor.new("Bright green"))
        game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(0.5, 1, 0.5), Vector3.new(0.5, 1, 0.5), BrickColor.new("Bright green"))
        game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(0.3, 1, 0.3), Vector3.new(0.3, 1, 0.3), BrickColor.new("Bright green"))
        game.ReplicatedStorage.Damage12:FireServer(human, hrp.CFrame, 0, .1, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 300 + Vector3.new(0, 0, 0), 0, "", 1, 1)
        wait(0.1)
        _G.specialdashmove()
        wait(0.7)
        Humanoid.WalkSpeed = 20
        Humanoid.JumpPower = 50 
        noantifling = false
        wait(0.5)
        active = false
    end 
end)
-----------------------------------------------
_G.specialdashmove2 = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character["Left Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character["Left Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character["Left Arm"].Size * 2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.1) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

_G.specialdashmove3 = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character["Right Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character["Right Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character["Right Arm"].Size * 2
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.1) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage2:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(hit.Parent.Humanoid, hit.Parent.Torso.CFrame, 10, 0.1, Vector3.new(0, 0, 0), 0.1, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

local specialdashcount = 0
mouse.KeyDown:connect(function(key)
	if key == "t" then
    if active == true then return end
    if loading == true then return end
	if attackmode == true then return end
	if standmodecooler == false then return end
    if death == true then return end
		active = true
		_G.anim23:Play(0.1,1,1.3)
        game.ReplicatedStorage.Damage12:FireServer(human, hrp.CFrame, 0, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "rbxassetid://4594155088", 1, 1)
		wait(0.15)
		repeat
			specialdashcount = specialdashcount + 1
			_G.specialdashmove3()
			_G.specialdashmove2()
			wait(0.05)
		until specialdashcount == 10
		specialdashcount = 0
		wait(0.5)
		active = false
	end
end)
