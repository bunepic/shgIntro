--[[mouse.KeyDown:Connect(function(key)
    if key == 'b' then
	if death == true then return end
	if secretmodeon == false then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
		active = true
        noantifling = true
        human:ChangeState(Enum.HumanoidStateType.Jumping)
        wait()
        human:ChangeState(Enum.HumanoidStateType.Jumping)
        wait()
        human:ChangeState(Enum.HumanoidStateType.Jumping)
        wait()
        human:ChangeState(Enum.HumanoidStateType.Jumping)
        game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0.1, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 75, 0, "", 1, 1)
        _G.anim38:Play()
        noantifling = false
        wait(0.5)
        repeat
		wait()
		kcauslidecount = kcauslidecount + 1
		_G.kcauslidehitbox()
	until human:GetState(Enum.HumanoidStateType.Landed) or kcauslidecount == 45
        _G.anim38:Stop()
        kcauslidecount = 0
	noantifling = false
        wait(0.15)
	noantifling = false
        active = false
    end 
end) ]]--

mouse.KeyDown:Connect(function(key)
    if key == '1' then
	if death == true then return end
	if secretmodeon == false then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
	if mouse.Target and mouse.Target.Parent:FindFirstChild("Humanoid") then
	local horp = mouse.Target.Parent:FindFirstChild("HumanoidRootPart")
	local magnitude = (horp.Position - hrp.Position).magnitude
	local ezkill = mouse.Target.Parent
	active = true
	human.AutoRotate = false
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Torso"],true)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Head"],true)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Right Arm"],true)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Left Arm"],true)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Right Leg"],true)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Left Leg"],true)
	wait()
	human.WalkSpeed = 0
	human.JumpPower = 0
	_G.anim29:Play(0.1,1,500)
	for _ = 1, 100 do
	    wait()
	    hrp.CFrame = ezkill.HumanoidRootPart.CFrame * CFrame.new(0,0,2.5)
	    game:GetService("ReplicatedStorage").Damage2:FireServer(ezkill.Humanoid, ezkill["Head"].CFrame, 0.5, 0, Vector3.new(0, 0, 0), 0.075, "", 1, 1)
	    game:GetService("ReplicatedStorage").Damage2:FireServer(ezkill.Humanoid, ezkill["Head"].CFrame, 0.5, 0, Vector3.new(0, 0, 0), 0.075, "", 1, 1)
	    game:GetService("ReplicatedStorage").ULFDamage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Left Arm"].CFrame, 0, 0, Vector3.new(0, 0, 0), 0, "", 1, 1)
	    game:GetService("ReplicatedStorage").ULFDamage:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame, 0, 0, Vector3.new(0, 0, 0), 0, "", 1, 1)
	end
	for _ = 1, 25 do
	    game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Head"].CFrame, 0, 0, Vector3.new(0, 0, 0), 1, "", 1, 1)
	    game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Head"].CFrame, 0, 0, Vector3.new(0, 0, 0), 1.25, "", 1, 1)
	    game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Head"].CFrame, 0, 0, Vector3.new(0, 0, 0), 1.5, "", 1, 1)
	end
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Torso"],false)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Head"],false)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Right Arm"],false)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Left Arm"],false)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Right Leg"],false)
	game:GetService("ReplicatedStorage").Anchor:FireServer(ezkill["Left Leg"],false)
	_G.anim29:Stop()
	wait()
	active = false
	human.AutoRotate = true
	human.WalkSpeed = 12
	human.JumpPower = 50
	end
end
end)
-----------------
_G.secretmodeF = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].Size * 1.5
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(255,255,255)
    game.Debris:AddItem(hitbox1, 0.2) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
            hitbox1:Destroy()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                delay(.001, function() debounce = false end)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
	    	game:GetService("ReplicatedStorage").Damage12Sans:FireServer(hit.Parent.Humanoid, hit.Parent["Torso"].CFrame, 15, 0.1, Vector3.new(0, 25, 0), 0.075, "", 1, 1)
            hitbox1:Destroy()
            end
        end
    end)
end

_G.secretmodeFtwo = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Left Arm"].Size * 1.5
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(255,255,255)
    game.Debris:AddItem(hitbox1, 0.3) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
