	    	local limbuchoppa2 = 
{
	    	[1] = hit.Parent.Parent.Humanoid, 
	    	[2] = hit.Parent.Parent.Torso.CFrame
}
	    	active = true
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            hitbox1:Destroy()
            game:GetService("ReplicatedStorage").Damage11:FireServer(hit.Parent.Parent.Humanoid, hit.Parent.Parent.Torso.CFrame, 99, 0, Vector3.new(0,0,0), 1, "rbxassetid://2320331820", 1, 1)
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                print(hit.Parent.Name .. " elif")
                delay(.001, function() debounce = false end)
	    	local limbuchoppa = "limbchop"
	    	local limbuchoppa2 = 
{
	    	[1] = hit.Parent.Parent.Humanoid, 
	    	[2] = hit.Parent.Parent.Torso.CFrame
}
	    	active = true
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            hitbox1:Destroy()
            game:GetService("ReplicatedStorage").Damage11:FireServer(hit.Parent.Parent.Humanoid, hit.Parent.Parent.Torso.CFrame, 99, 0, Vector3.new(0,0,0), 1, "rbxassetid://2320331820", 1, 1)
            end
        end
    end)
end
-------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == 'q' then
	if death == true then return end
	if standmodecooler == true then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
		trans0()
		active = true
        _G.anim18:Play(0.1, 1, 2.5)
        game.ReplicatedStorage.Damage12:FireServer(human, hrp.CFrame, 0, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "rbxassetid://4474239559", 1, 1)
        wait(0.3)
        _G.standslash1()
        wait(0.3)
        _G.standslash2()
		wait(0.3)
        _G.standslash1()
		trans1()
        wait(1)
        active = false
    end 
end) 
-----------------------------------------------
_G.legslam = function()
    game.ReplicatedStorage.DoppioSlam:FireServer()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 35 then
                game.ReplicatedStorage.Knock:FireServer(v.Humanoid)
                game.ReplicatedStorage.KQRGrab:FireServer(v.Humanoid)
                mesplash:FireServer()
                mesplash:FireServer()
            end
        end
    end
end
-----------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == 'g' then
	if death == true then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == false then return end
	if specmodecooler == true then return end
        active = true
		_G.anim19:Play(0.1,1,1.3)
        wait(0.3)
        _G.legslam()
        wait(1)
        active = false
    end 
end) 
-----------------------------------------------
_G.bringall = function()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.huge then
                game.ReplicatedStorage.KQRGrab:FireServer(v.Humanoid)
            end
        end
    end
end
-----------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == '5' then
	if death == true then return end
    if loading == true then return end
	if active == true then return end
        _G.bringall()
    end 
end) 
-----------------------------------------------
local deathevent = replicatedstorage.Death
human.HealthChanged:connect(function()
	if human.Health < 1 then
		human:SetStateEnabled(3, false)
		human:SetStateEnabled(15, false)
		attackmode = false
		death = true
		player.ChatColor.G.Value = 255
		player.ChatColor.R.Value = 255
		player.ChatColor.B.Value = 255
		player.ChatColor.BlackWhite.Value = true
		print(attackmode)
		print(human.MaxHealth)
		deathevent:FireServer(false)
	end
end)
-----------------------------------------------
local rStorage = game:GetService("ReplicatedStorage")
local Player = game:GetService("Players").LocalPlayer

local AuraSetter = 0
local rtzauractive = false
game.Lighting.TS.Changed:Connect(function()
    if death == true then return end
    if attackmode == false then
    if game.Lighting.TS.Value == true then
        rtzauractive = true
        tauntactive = true
        game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://2540377045", 0,3.5, 0)
        trans0()
        _G.anim17:Play()
        rtzevent:FireServer(true)
        game:GetService("ReplicatedStorage").RTZEffect:FireServer(true)
        coroutine.resume(coroutine.create(function()
        repeat
            AuraSetter = AuraSetter + 1
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*-5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*0) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            wait()
        until human.Health < 0.001  or rtzauractive == false
        end))
        wait(2)
        trans1()
        _G.anim17:Stop()
        rtzevent:FireServer(false)
        wait(1.5)
        rtzauractive = false
        wait(1)
        tauntactive = false
        end
    end
end)

game.Lighting.TE.Changed:Connect(function()
    if death == true then return end
    if attackmode == false then
    if game.Lighting.TE.Value == true then
        rtzauractive = true
        tauntactive = true
        game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://2540377045", 0,3.5, 0)
        trans0()
        _G.anim17:Play()
        rtzevent:FireServer(true)
        game:GetService("ReplicatedStorage").RTZEffect:FireServer(true)
        coroutine.resume(coroutine.create(function()
        repeat
            AuraSetter = AuraSetter + 1
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*-5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*5, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*0) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            rStorage.Damage11Enderman:FireServer(Player.Character.Humanoid, human.Parent.HumanoidRootPart.CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*5, math.sin(AuraSetter * 50)*5) , 0, 0, nil, 0, "rbxassetid://2835079178", 1, 0)
            wait()
        until human.Health < 0.001  or rtzauractive == false
        end))
        wait(2)
        trans1()
        _G.anim17:Stop()
        rtzevent:FireServer(false)
        wait(1.5)
        rtzauractive = false
        wait(1)
        tauntactive = false
        end
    end
end)

