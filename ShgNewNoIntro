            end
        end
    end)
end

mouse.KeyDown:Connect(function(key)
    if key == 'f' then
	if death == true then return end
	if secretmodeon == false then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
		active = true
		trans0()
		_G.anim4:Play(0.1,1,2)
		coroutine.resume(coroutine.create(function()
		wait(0.07)
		_G.secretmodeF()
		end))
		repeat wait() until _G.anim4.TimePosition > 0.5
		_G.anim4:Stop()
		_G.anim22:Play(0.1,1,1.3)
		_G.anim22.TimePosition = 0.5
		coroutine.resume(coroutine.create(function()
		wait(0.1)
		_G.secretmodeFtwo()
		end))
		coroutine.resume(coroutine.create(function()
		wait(0.07)
		trans1()
		end))
		repeat wait() until _G.anim22.TimePosition > 1.2
		_G.anim22:Stop()
		wait(0.35)
		active = false
	end
end)
