mouse.KeyDown:connect(function(key)
	if key == "r" then
    if active == true then return end
	if death == true then return end
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		active = true
		_G.anim30:Play()
		wait(0.6)
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-1,-0.1,-1), Vector3.new(-1,-0.1,-1), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-2,-0.2,-2), Vector3.new(-2,-0.2,-2), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-3,-0.3,-3), Vector3.new(-3,-0.3,-3), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-4,-0.4,-4), Vector3.new(-4,-0.4,-4), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-5,-0.5,-5), Vector3.new(-5,-0.5,-5), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-5,0.5,-5), Vector3.new(-5,0.5,-5), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-4,0.5,-4), Vector3.new(-4,0.5,-4), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-3,0.5,-3), Vector3.new(-3,0.5,-3), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-2,0.5,-2), Vector3.new(-2,0.5,-2), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-1,0.5,-1), Vector3.new(-1,0.5,-1), BrickColor.new("Bright green"))
		_G.hoomanR4()
		wait(0.5)
		active = false
	end
end)
