function switchmodetwo()
if attackmode == true then return end
if secretmodeon == true then return end
if specmodecooler == true then return end
if standmodecooler == true then
	standmodecooler = false
else
	standmodecooler = true
end	
end

mouse.KeyDown:connect(function(key)
    if key == "-" then
    if loading == true then return end
    if death == true then return end
    if specmodecooler == true then return end
    if secretmodeon == true then return end
        game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
        switchmodetwo()
    end
end)
---------------------------------
function switchmodethird()
if attackmode == false then return end
if standmodecooler == true then return end
if secretmodeon == true then return end
if specmodecooler == true then
	specmodecooler = false
else
	specmodecooler = true
end	
end

mouse.KeyDown:connect(function(key)
    if key == "-" then
    if loading == true then return end
    if death == true then return end
    if standmodecooler == true then return end
    if secretmodeon == true then return end
        game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
        switchmodethird()
    end
end)
