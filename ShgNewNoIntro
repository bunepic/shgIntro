function attackmodetrail()
if death == true then return end
if attackmode == true then
if secretmodeon == true then return end
if standmodecooler == true then return end
if specmodecooler == true then return end
	repeat
		wait()
		game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Pastel Blue"))
		game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, nil, 0.008, "", 1, 1)
		wait(0.1)
		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, nil, 0.008, "", 1, 1)
		game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Tr. Red"))
	until attackmode == false
end
end
---------------------------------
local dmgtrail = false

function damagetrail()
if death == true then return end
if attackmode == false then
if secretmodeon == true then return end
	dmgtrail = true
	repeat
		wait(0.1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Left Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Right Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Left Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Torso"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Torso"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Head"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Head"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
	until attackmode == true or dmgtrail == false
end
end
---------------------------------
function switchmode()
if attackmode == true then
if death == true then return end
if secretmodeon == true then return end
if standmodecooler == true then return end
if specmodecooler == true then return end
	attackmode = false
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 26
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = 55
else
	attackmode = true
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 34
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = 60
	attackmodetrail()
end	
end

mouse.KeyDown:connect(function(key)
    if key == "x" then
    if loading == true then return end
    if death == true then return end
    if secretmodeon == true then return end
    if standmodecooler == true then return end
    if specmodecooler == true then return end
        game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
        switchmode()
    end
end)
---------------------------------
function passivedodgeanimate()
	local randompassivedodgeanimate = math.random(1, 2)
	if randompassivedodgeanimate == 1 then
_G.anim39:Play()
	elseif randompassivedodgeanimate == 2 then
_G.anim40:Play()
		end
end

function secretmode()
if standmodecooler == true then return end
if death == true then return end
if secretmodeon == true then return end
if attackmode == false then
    active = true
    anotherrtz = true
	secretmodeon = true
	chr.Animate.walk.WalkAnim.AnimationId = "rbxassetid://5166114330"
	chr.Animate.fall.FallAnim.AnimationId = "rbxassetid://5166133986" 
	chr.Animate.jump.JumpAnim.AnimationId = "rbxassetid://2763931895" 
	chr.Animate.idle.Animation1.AnimationId = "rbxassetid://5166116703"
	human.WalkSpeed = 0
	human.JumpPower = 0
	_G.anim35:Play()
	wait(0.33)
	for _ = 1,100 do
		wait()
		doppioslam:FireServer()
		game:GetService("ReplicatedStorage").Damage11Enderman:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-6,-0.4,-6), Vector3.new(-6,-0.4,-6), BrickColor.new("Transparent"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-5,-0.4,-5), Vector3.new(-5,-0.4,-5), BrickColor.new("Transparent"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-4,-0.4,-4), Vector3.new(-4,-0.4,-4), BrickColor.new("Transparent"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-3,-0.4,-3), Vector3.new(-3,-0.4,-3), BrickColor.new("Transparent"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-2,-0.4,-2), Vector3.new(-2,-0.4,-2), BrickColor.new("Transparent"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-1,-0.4,-1), Vector3.new(-1,-0.4,-1), BrickColor.new("Transparent"))
	end
	rStorage.RTZ:FireServer(true)
	wait(1.5)
	active = false
	human.WalkSpeed = 12
	human.JumpPower = 50
end
end

mouse.KeyDown:connect(function(key)
    if key == "=" then
    if death == true then return end
        secretmode()
    end
end)
