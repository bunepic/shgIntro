local niggacount = 0

mouse.KeyDown:Connect(function(key)
    if key == '1' then
    if active == true then return end
	if death == true then return end
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
	    if mouse.Target and mouse.Target.Parent:FindFirstChild("Humanoid") then
	    	active = true
	    	local horp = mouse.Target.Parent:FindFirstChild("HumanoidRootPart")
	    	local magnitude = (horp.Position - hrp.Position).magnitude
	    	local ezkill = mouse.Target.Parent
            game.ReplicatedStorage.HamonSword:FireServer(ezkill.Humanoid, 1, math.huge, Vector3.new(0,0,0))
            game.ReplicatedStorage.SamuraiDamage2:FireServer(ezkill.Humanoid, 1, math.huge, Vector3.new(0,0,0))
	    	repeat
	    	    game:GetService("ReplicatedStorage").Damage12Sans:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 0, 20, Vector3.new(0, 0, 0), 0.5, "", 1, 1)
