function attackmodetrail()
if death == true then return end
if attackmode == true then
if standmodecooler == true then return end
if specmodecooler == true then return end
	repeat
		wait()
		game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Pastel Blue"))
		game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 0.008, "", 1, 1)
		wait(0.1)
		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 0.008, "", 1, 1)
		game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Tr. Red"))
	until attackmode == false
end
end
---------------------------------
local dmgtrail = false

function damagetrail()
if death == true then return end
if attackmode == false then
	dmgtrail = true
	repeat
		wait(0.1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Left Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Right Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Left Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Torso"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Torso"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
		game.ReplicatedStorage.Damage:FireServer(human, chr["Head"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Head"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", .1, Color3.new(0, 255, 0), "", 1, 1)
	until attackmode == true or dmgtrail == false
end
end
---------------------------------
function switchmode()
if attackmode == true then
if death == true then return end
if standmodecooler == true then return end
if specmodecooler == true then return end
	attackmode = false
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 26
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = 55
else
	attackmode = true
	game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = 34
	game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = 60
	attackmodetrail()
end	
end

mouse.KeyDown:connect(function(key)
    if key == "x" then
    if loading == true then return end
    if death == true then return end
    if standmodecooler == true then return end
    if specmodecooler == true then return end
        game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
        switchmode()
    end
end)
---------------------------------
function switchmodetwo()
if attackmode == true then return end
if specmodecooler == true then return end
if standmodecooler == true then
	standmodecooler = false
else
	standmodecooler = true
end	
end

mouse.KeyDown:connect(function(key)
    if key == "-" then
    if loading == true then return end
    if death == true then return end
    if specmodecooler == true then return end
        game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
        switchmodetwo()
    end
end)
---------------------------------
function switchmodethird()
if attackmode == false then return end
if standmodecooler == true then return end
if specmodecooler == true then
	specmodecooler = false
else
	specmodecooler = true
end	
end

