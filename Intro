-------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == 'q' then
	if death == true then return end
	if standmodecooler == true then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == true then return end
		trans0()
		active = true
        _G.anim18:Play(0.1, 1, 2.5)
        game.ReplicatedStorage.Damage12:FireServer(human, hrp.CFrame, 0, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "rbxassetid://4474239559", 1, 1)
        wait(0.3)
        _G.standslash1()
        wait(0.3)
        _G.standslash2()
		wait(0.3)
        _G.standslash1()
		trans1()
        wait(1)
        active = false
    end 
end) 
-----------------------------------------------
_G.legslam = function()
    game.ReplicatedStorage.DoppioSlam:FireServer()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 35 then
                game.ReplicatedStorage.Knock:FireServer(v.Humanoid)
                game.ReplicatedStorage.KQRGrab:FireServer(v.Humanoid)
                mesplash:FireServer()
                mesplash:FireServer()
            end
        end
    end
end
-----------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == 'g' then
	if death == true then return end
    if loading == true then return end
	if active == true then return end
	if attackmode == false then return end
	if specmodecooler == true then return end
        active = true
		_G.anim19:Play(0.1,1,1.3)
        wait(0.3)
        _G.legslam()
        wait(1)
        active = false
    end 
end) 
-----------------------------------------------
_G.bringall = function()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.huge then
                game.ReplicatedStorage.KQRGrab:FireServer(v.Humanoid)
            end
        end
    end
end
-----------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == '5' then
	if death == true then return end
    if loading == true then return end
	if active == true then return end
        _G.bringall()
    end 
end) 
-----------------------------------------------
local deathevent = replicatedstorage.Death
human.HealthChanged:connect(function()
	if human.Health < 1 then
		human:SetStateEnabled(3, false)
		human:SetStateEnabled(15, false)
		attackmode = false
		death = true
		player.ChatColor.G.Value = 255
		player.ChatColor.R.Value = 255
		player.ChatColor.B.Value = 255
		player.ChatColor.BlackWhite.Value = true
		print(attackmode)
		print(human.MaxHealth)
		deathevent:FireServer(false)
	end
end)
