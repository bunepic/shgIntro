game.Lighting.TS.Changed:Connect(function()
    if death == true then return end
    if attackmode == false then
    if game.Lighting.TS.Value == true then
        tauntactive = true
        game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://2540377045", 0,3.5, 0)
        trans0()
        _G.anim17:Play()
        rtzevent:FireServer(true)
        game:GetService("ReplicatedStorage").RTZEffect:FireServer(true)
        wait(2)
        trans1()
        _G.anim17:Stop()
        rtzevent:FireServer(false)
        wait(2.5)
        tauntactive = false
        end
    end
end)

