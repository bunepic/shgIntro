mouse.KeyDown:Connect(function(key)
    if key == '1' then
	    if attackmode == true then return end
	    if standmodecooler == true then return end
        if loading == true then return end
	    if death == true then return end
	    if donutactive == true then return end
	    if mouse.Target and mouse.Target.Parent:FindFirstChild("Humanoid") then
	    	local horp = mouse.Target.Parent:FindFirstChild("HumanoidRootPart")
	    	local magnitude = (horp.Position - hrp.Position).magnitude
	    	local ezkill = mouse.Target.Parent
	    	local limbuchoppa = "limbchop"
	    	local limbuchoppa2 = 
{
	    	[1] = ezkill.Humanoid, 
	    	[2] = ezkill.Torso.CFrame
}
	    	active = true
            game.ReplicatedStorage.Damage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 0, 0.6, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(0, 255, 0), "rbxassetid://321398701", 1, 1)
            chr.HumanoidRootPart.CFrame = ezkill.HumanoidRootPart.CFrame  * CFrame.new(6, 0, 0)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").TwentyMeterSplash:FireServer()
            wait(0.2)
            chr.HumanoidRootPart.CFrame = ezkill.HumanoidRootPart.CFrame  * CFrame.new(0, 0, -6)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").TwentyMeterSplash:FireServer()
            wait(0.2)
            chr.HumanoidRootPart.CFrame = ezkill.HumanoidRootPart.CFrame  * CFrame.new(-6, 0, 0)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").TwentyMeterSplash:FireServer()
            wait(0.2)
            chr.HumanoidRootPart.CFrame = ezkill.HumanoidRootPart.CFrame  * CFrame.new(0, 0, 6)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            game:GetService("ReplicatedStorage").TwentyMeterSplash:FireServer()
	        active = false
	    end
    end
end)
