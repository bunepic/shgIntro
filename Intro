local tauntactive = false
function taunt()
	if tauntactive == true then return end
	tauntactive = true
	local randomquote = math.random(1, 23)
	if randomquote == 1 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://3651529648", 0,3.5, 0)
	elseif randomquote == 2 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://3651526876", 0,3.5, 0)
	elseif randomquote == 3 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://5569743507", 0,3.5, 0)
	elseif randomquote == 4 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://5569742638", 0,3.5, 0)
	elseif randomquote == 5 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://5569744580", 0,3.5, 0)
	elseif randomquote == 6 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://5571408417", 0,3.5, 0)
	elseif randomquote == 7 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://1280294411", 0,3.5, 0)
	elseif randomquote == 8 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://4693804265", 0,3.5, 0)
	elseif randomquote == 9 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://4686410979", 0,3.5, 0)
	elseif randomquote == 10 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://4686410317", 0,3.5, 0)
	elseif randomquote == 11 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://4654042242", 0,3.5, 0)
	elseif randomquote == 12 then
game:GetService("ReplicatedStorage").Taunt:FireServer(chr.Head, "rbxassetid://4464243333", 0,3.5, 0)
