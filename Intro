-------------------------------------------
_G.grabactive = function()
    wait(3)
    active = false
end
-------------------------------------------
mouse.KeyDown:Connect(function(key)
    if key == 'q' then
	if attackmode == false then return end
	if specmodecooler == true then return end
	if death == true then return end
	if active == true then return end
		active = true
        _G.anim13:Play()
        wait(0.4)
        _G.grab()
        wait(0.1)
	if grabbing == true then
        game.ReplicatedStorage.Damage12:FireServer(human, hrp.CFrame, 0, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "rbxassetid://4594155998", 1, 1)
        _G.anim14:Play()
        wait(1)
        _G.anim15:Play()
        wait(0.3)
        _G.headhitdoe()
        wait(0.2)
        _G.anim14:Stop()
        wait(0.7)
		active = false
        grabbing = false
		print(grabbing)
	elseif grabbing == false then
        wait(0.4)
		active = false
		print(grabbing)
    end 
end
end)
