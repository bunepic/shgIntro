            chr.HumanoidRootPart.CFrame = hit.Parent.HumanoidRootPart.CFrame  * CFrame.new(1, 4, 0)
            _G.anim19:Play(0.1,1,2)
            wait(0.1)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 175, .1, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 40 + Vector3.new(0, -150, 0), "rbxassetid://241837157", .1, Color3.new(0, 255, 0), "", 1, 1)
            end
