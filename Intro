            hitbox1:Destroy()
            local limbuchoppa = "limbchop"
            local limbuchoppa2 = 
{
            [1] = hit.Parent.Humanoid, 
            [2] = hit.Parent.Torso.CFrame
}
            wait(0.1)
            game:GetService("ReplicatedStorage").Damage11:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 999, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage11:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 999, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage11:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 999, "", 1, 1)
            chr.HumanoidRootPart.CFrame = hit.Parent.HumanoidRootPart.CFrame  * CFrame.new(0, 0, 6)
            game:GetService("ReplicatedStorage").MadeInHeavenHandler:FireServer(limbuchoppa,limbuchoppa2)
            wait(0.1)
            game:GetService("ReplicatedStorage").Damage11:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 999, "", 1, 1)
