_G.rMoveLol = function()
        
    -- make hitbox part
    local hitbox1 = Instance.new("Part", game.Players.LocalPlayer.Character)
    hitbox1.CFrame = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].CFrame
    hitbox1.Name = "hitbox1"
    hitbox1.Transparency = 1
    hitbox1.Anchored = false
    hitbox1.CanCollide = false

    -- make weld that connects to part and hitbox
    local weld = Instance.new("WeldConstraint", hitbox1)
    weld.Part0 = hitbox1
    weld.Part1 = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"]

    hitbox1.Size = game.Players.LocalPlayer.Character.Stand["Stand Right Arm"].Size * 1.5
    hitbox1.Massless = true
    hitbox1.Color = Color3.new(0,0,0)
    game.Debris:AddItem(hitbox1, 0.5) -- destroy hitbox after .3 seconds (with no code yielding)

    --[[
        local userdata_1 = game:GetService("Workspace").Entities["Menacing Dummy"].Humanoid;  target
        local userdata_2 = CFrame.new(-6266.57568, 576.63147, -389.268402, 0.910382628, -0.322563976, 0.259144157, 0.282751679, 0.0277367681, -0.958792031, 0.30208388, 0.946141243, 0.116456524);  punch effect position
        local number_1 = 50;  damage
        local number_2 = 0.25;  velocity duration
        local userdata_3 = Vector3.new(45.6050453, -0.000103456521, -20.4982872);  velocity
        local string_1 = "rbxassetid://241837157";  punch effect
        local number_3 = 0.075;  punch effect size
        local userdata_4 = Color3.new(0, 255, 0);  color
        local string_2 = "rbxassetid://260430079";  sound
        local number_4 = 0.9;  playback speed
        local number_5 = 1;  punch sound volume
        local Target = game:GetService("ReplicatedStorage").Damage;
        Target:FireServer(userdata_1, userdata_2, number_1, number_2, userdata_3, string_1, number_3, userdata_4, string_2, number_4, number_5);
    ]]
    
    local debounce = false
    
    -- deal damage if arm hits a humanoid
    local hitRegister = hitbox1.Touched:Connect(function(hit)
        if hit.Parent:FindFirstChild("Humanoid") and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                print(hit.Parent.Name)
                delay(.001, function() debounce = false end)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 170, .03, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 300 + Vector3.new(0, 30, 0), "rbxassetid://241837157", .25, Color3.new(0, 255, 0), "rbxassetid://367499850", 1, 1)
            hitbox1:Destroy()
            wait(0.4)
            chr:SetPrimaryPartCFrame(hit.Parent.Torso.CFrame * CFrame.new(0, 0, -3))
            trans0()
            _G.anim4:Play(0.1, 1, 0.6)
            wait(0.2)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 170, .05, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 100 + Vector3.new(0, 75, 0), "rbxassetid://241837157", .45, Color3.new(0, 255, 0), "rbxassetid://367499850", 1, 1)
            game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(2, 0.5, 2), Vector3.new(2, 0.5, 2), BrickColor.new("Bright green"))
            game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(1, 0.5, 1), Vector3.new(1, 0.5, 1), BrickColor.new("Bright green"))
            wait(0.2)
            trans1()
            wait(0.2)
            _G.anim4:Stop()
            end
        elseif(hit.Parent.Parent:FindFirstChild("Humanoid")) and hit.Parent.Name ~= game.Players.LocalPlayer.Name then
                if debounce == false then
                debounce = true
                print(hit.Parent.Name .. " elif")
                delay(.001, function() debounce = false end)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Parent.Humanoid, hit.Parent.Parent.HumanoidRootPart.CFrame, 170, .03, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 30 + Vector3.new(0, 30, 0), "rbxassetid://241837157", .25, Color3.new(0, 255, 0), "", 1, 1)
            hitbox1:Destroy()
            wait(0.4)
            chr:SetPrimaryPartCFrame(hit.Parent.Torso.CFrame * CFrame.new(0, 0, -3))
            trans0()
            _G.anim4:Play(0.1, 1, 0.6)
            wait(0.2)
            game.ReplicatedStorage.Damage:FireServer(hit.Parent.Humanoid, hit.Parent.HumanoidRootPart.CFrame, 170, .05, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 100 + Vector3.new(0, 75, 0), "rbxassetid://241837157", .45, Color3.new(0, 255, 0), "rbxassetid://367499850", 1, 1)
            game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(2, 0.5, 2), Vector3.new(2, 0.5, 2), BrickColor.new("Bright green"))
            game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(1, 0.5, 1), Vector3.new(1, 0.5, 1), BrickColor.new("Bright green"))
            wait(0.2)
            trans1()
            wait(0.2)
            _G.anim4:Stop()
            end
        end
    end)
end

mouse.KeyDown:Connect(function(key)
    if key == 'r' then
	if active == true then return end
	if death == true then return end
	if attackmode == true then return end
	if standmodecooler == true then return end
    if loading == true then return end
	if RMoveCd == true then return end
        trans0()
        active = true
        game:GetService("ReplicatedStorage").ParticleEvent:FireServer("aura")
        RMoveCd = true
        _G.anim6:Play()
        game:GetService("ReplicatedStorage").ParticleEvent:FireServer("noaura")
        wait(0.6)
        _G.rMoveLol()
        game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(0.1, 0.6, 0.1), Vector3.new(1, 0.6, 1), BrickColor.new("Bright green"))
        game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(0.9, 0.5, 0.9), Vector3.new(0.9, 0.5, 0.9), BrickColor.new("Bright green"))
        game:GetService("ReplicatedStorage").StrongPunch:FireServer(Vector3.new(0.7, 0.3, 0.7), Vector3.new(0.7, 0.3, 0.7), BrickColor.new("Bright green"))
        trans1()
        active = false
        wait(0.4)
        RMoveCd = false
    end
end)
