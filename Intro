local count = 0
local donutactive = false

mouse.KeyDown:Connect(function(key)
    if key == '1' then
	    if attackmode == false then return end
      	if specmodecooler == true then return end
        if loading == true then return end
	    if death == true then return end
	    if donutactive == true then return end
	    if mouse.Target and mouse.Target.Parent:FindFirstChild("Humanoid") then
	    	local horp = mouse.Target.Parent:FindFirstChild("HumanoidRootPart")
	    	local magnitude = (horp.Position - hrp.Position).magnitude
	    	local ezkill = mouse.Target.Parent
	    	donutactive = true
            game.ReplicatedStorage.Damage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 0, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 300 + Vector3.new(0, 200, 0), "rbxassetid://241837157", 0, Color3.new(0, 255, 0), "rbxassetid://948494432", 1, 1)
            _G.anim16:Play()
            _G.anim16:AdjustSpeed(0)
	    	repeat
	        	count = count + 1
	        	chr.HumanoidRootPart.CFrame = ezkill.HumanoidRootPart.CFrame  * CFrame.new(0, 0, 3)
	    	    wait(0.1)
	        until count == 15
            _G.anim16:Stop()
	        game:GetService("ReplicatedStorage").BurnDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "", math.random(98,106)/100, 7.5)
	    	game.ReplicatedStorage.SamuraiDamage2:FireServer(ezkill.Humanoid,math.huge)
	    	game:GetService("ReplicatedStorage").ULFDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://5382576606", 1, 1)
	        game:GetService("ReplicatedStorage").BurnDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "", math.random(98,106)/100, 7.5)
	    	game.ReplicatedStorage.SamuraiDamage2:FireServer(ezkill.Humanoid,math.huge)
	    	game:GetService("ReplicatedStorage").ULFDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://5382576606", 1, 1)
	        game:GetService("ReplicatedStorage").BurnDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "", math.random(98,106)/100, 7.5)
	    	game.ReplicatedStorage.SamuraiDamage2:FireServer(ezkill.Humanoid,math.huge)
	    	game:GetService("ReplicatedStorage").ULFDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://5382576606", 1, 1)
	        game:GetService("ReplicatedStorage").BurnDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "", math.random(98,106)/100, 7.5)
	    	game.ReplicatedStorage.SamuraiDamage2:FireServer(ezkill.Humanoid,math.huge)
	    	game:GetService("ReplicatedStorage").ULFDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 99, 0, Vector3.new(0, 0, 0), 1, "rbxassetid://5382576606", 1, 1)
	        game:GetService("ReplicatedStorage").BurnDamage:FireServer(ezkill.Humanoid, ezkill.HumanoidRootPart.CFrame, 9e999, 0, Vector3.new(0, 0, 0), "rbxassetid://241837157", 0, Color3.new(255, 255, 255), "", math.random(98,106)/100, 7.5)
