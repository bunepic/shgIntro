_G.omtohScream = function()
    game:GetService("ReplicatedStorage").RTZEffect:FireServer(true)
    yell:FireServer()
    wait(0.275)
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(1, -0.3, 1), Vector3.new(1, -0.3, 1), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(2, -0.5, 2), Vector3.new(2, -0.5, 2), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(3, -0.7, 3), Vector3.new(3, -0.7, 3), BrickColor.new("Bright green"))
    game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(4, -1, 4), Vector3.new(4, -1, 4), BrickColor.new("Bright green"))
    doppioslam:FireServer()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < 50 then
            game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game.ReplicatedStorage.SamuraiDamage2:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage12Sans:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            game:GetService("ReplicatedStorage").Damage3:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
            end
        end
    end
end
