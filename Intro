game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
game:GetService("ReplicatedStorage").PlayerStrongPunch2:FireServer(Vector3.new(-femboy,-1,-femboy), Vector3.new(-femboy,-1,-femboy), BrickColor.new("Bright green"))
femboy = femboy + 1
femboy = 1
end

_G.humanityDestroy = function()
    fatfuck()
    doppioslam:FireServer()
    game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 0, "rbxassetid://3673187353", 2, 10)
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.huge then
            game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game.ReplicatedStorage.SamuraiDamage2:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 1.5, "", 1, 1)
            end
        end
    end
end

local nukedestroy = false

mouse.KeyDown:connect(function(key)
	if key == "4" then
    if nukedestroy == true then return end
    if loading == true then return end
	if attackmode == true then return end
	if standmodecooler == false then return end
    if death == true then return end
        nukedestroy = true
		human.WalkSpeed = 0
		human.JumpPower = 0
		_G.anim25:Play()
		wait(0.2)
		_G.humanityDestroy()
		wait(0.35)
		human.WalkSpeed = 20
		human.JumpPower = 50
		wait(1)
        nukedestroy = false
	end
end)
-----------------------------------------------
local rStorage = game:GetService("ReplicatedStorage")
local Player = game:GetService("Players").LocalPlayer
local Move = false
local Mouse = Player:GetMouse()

local Countering = false
function Counter()
    pcall(function()
        if Player.Character.Parent == workspace.Entities then
            if not Player.Character:FindFirstChild("Humanoid") then
                return
            end
        else
            return
        end
        Move = true
        Countering = true
        Player.Character.Humanoid.WalkSpeed = 0
        Player.Character.Humanoid.JumpPower = 0
        rStorage.RTZ:FireServer(true)
        _G.anim26:Play()
        trans0()
        game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Bright green"))
        repeat wait() until Countering == false
        _G.anim26:Stop()
        rStorage.RTZ:FireServer(false)
        Player.Character.Humanoid.WalkSpeed = 16
        Player.Character.Humanoid.JumpPower = 50
        Move = false
        trans1()
        game:GetService("ReplicatedStorage").VampireDash:FireServer(BrickColor.new("Bright green"))
    end)
end
Mouse.KeyUp:Connect(function(Key)
    if Key == "h" then
    if loading == true then return end
	if attackmode == true then return end
	if standmodecooler == false then return end
    if death == true then return end
        if Move == false and Countering == false then
            Counter()
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Left Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Left Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Torso"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Torso"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Head"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Head"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://241837157", 1, Color3.new(0, 255, 0), "", 1, 1)
        else
            Countering = false
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Left Arm"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Left Leg"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Right Arm"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Torso"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Torso"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Head"].CFrame, 0, 0, game.Players.LocalPlayer.Character["Head"].CFrame.LookVector * 0 + Vector3.new(0, 0, 0), "rbxassetid://3370684035", 1, Color3.new(0, 255, 0), "", 1, 1)
        end
    end
end)

rStorage.RTZClient.OnClientEvent:connect(function(gamer)
    if gamer ~= Player then
        local gmhum = gamer.Character.Humanoid
        Countering = false
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,3,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(1,2,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(-1,2,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,1,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,0,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(2,2,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(-2,2,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,-1,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,4,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,2,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        game.ReplicatedStorage.Damage:FireServer(human, hrp.CFrame * CFrame.new(0,-4,3), 0, 0, Vector3.new(0,0,0), "rbxassetid://241837157", 0, Color3.new(0,255,0), "rbxassetid://", 0, 0)
        Player.Character.HumanoidRootPart.CFrame = gamer.Character.HumanoidRootPart.CFrame  * CFrame.new(0, 0, 6)
        game.ReplicatedStorage.HamonSword:FireServer(gmhum, 1, math.huge, Vector3.new(0,math.huge,0))
        print(gamer)
    end
end)
-----------------------------------------------
function shooting()
	if death == true then return end
	if active == true then return end
	active = true
	local bep = true
	local de = mouse.KeyUp:connect(function(key)
	    if key == "g" then
	      bep = false
	    end
	end)
	_G.anim27:Play()
	repeat
		game:GetService("ReplicatedStorage").Damage12:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character["Right Arm"].CFrame, 0, 0, Vector3.new(0, 0, 0), 0.0055, "rbxassetid://846233576", 1, 1)
		game:GetService("ReplicatedStorage").EmperorBackdash:FireServer()
		wait(0.25)
	until bep == false
	_G.anim27:Stop()
	active = false
end
-----------------------------------------------
mouse.KeyDown:connect(function(key)
	if key == "g" then
    if loading == true then return end
	if attackmode == false then return end
	if specmodecooler == false then return end
		shooting()
	end
end)
